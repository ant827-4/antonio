fun main() {
    // conjunto apenas de leitura 
    val apenasleituraFrutas = setOf("abacate","banana","cereja","cereja")
    println(apenasleituraFrutas)
    
    // conjutos  mutavél com declaração de tipos explícita
    val frutas: MutableSet<String> = mutableSetOf("laranja", "tangerina","uva",)
    println(frutas)
   
    
    }
   
